{
  "kind": "collectionType",
  "collectionName": "experience",
  "info": {
    "singularName": "work",
    "pluralName": "experience",
    "displayName": "Work"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "company": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "location": {
      "type": "string"
    },
    "job_title": {
      "type": "string",
      "required": true
    },
    "start_year": {
      "type": "string",
      "required": true
    },
    "end_year": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "technologies": {
      "type": "text"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
